    "Server": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "\"$(VINTAGE_STORY)/VintagestoryServer.dll\" --tracelog --addModPath \"$(SolutionDir)/bin/$(Configuration)/Mods\" --dataPath \"$(SolutionDir)/data\"",
      "workingDirectory": "$(VINTAGE_STORY)"
    }
  }
}
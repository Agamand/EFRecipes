{
  "profiles": {
    "Client": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "\"$(VINTAGE_STORY)/Vintagestory.dll\" --tracelog --addModPath \"$(SolutionDir)/bin/$(Configuration)/Mods\" --addOrigin \"$(SolutionDir)/assets\" --dataPath \"$(SolutionDir)/data\"",
      "workingDirectory": "$(VINTAGE_STORY)"
